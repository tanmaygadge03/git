class TextEditor:
    def __init__(self):
        self.document = ""           # Current document state
        self.undo_stack = []         # Stack for undo
        self.redo_stack = []         # Stack for redo

    def make_change(self, new_text):
        self.undo_stack.append(self.document)  # Save current state before change
        self.document = new_text               # Apply new change
        self.redo_stack.clear()                # Clear redo history

    def undo(self):
        if self.undo_stack:
            self.redo_stack.append(self.document)
            self.document = self.undo_stack.pop()
        else:
            print("Nothing to undo.")

    def redo(self):
        if self.redo_stack:
            self.undo_stack.append(self.document)
            self.document = self.redo_stack.pop()
        else:
            print("Nothing to redo.")

    def display(self):
        print("Current Document State:", self.document)


# Interactive session
editor = TextEditor()

while True:
    print("\nOptions: [1] Make Change  [2] Undo  [3] Redo  [4] Display  [5] Exit")
    choice = input("Enter your choice: ")

    if choice == '1':
        new_text = input("Enter new document text: ")
        editor.make_change(new_text)
    elif choice == '2':
        editor.undo()
    elif choice == '3':
        editor.redo()
    elif choice == '4':
        editor.display()
    elif choice == '5':
        print("Exiting editor.")
        break
    else:
        print("Invalid choice. Please try again.")
