# Create an empty list
salaries = []

# Take 10 salaries as input
for i in range(10):
    a = float(input("Enter the Salary: "))
    salaries.append(a)

print("Original Salaries:", salaries)
salaries_copy=salaries.copy()

# Bubble Sort Function
def bubble_sort(sal):
    n = len(sal)
    for i in range(n - 1):
        for j in range(n - i - 1):
            if sal[j] > sal[j + 1]:
                sal[j], sal[j + 1] = sal[j + 1], sal[j]
    return (sal)
a=bubble_sort(salaries)
print("sorted by bubble sort:",a)

# Selection Sort Function
def selection_sort(sal):
    n = len(sal)
    for i in range(n - 1):
        min_index = i
        for j in range(i + 1, n):
            if sal[j] < sal[min_index]:
                min_index = j
        sal[i], sal[min_index] = sal[min_index], sal[i]
    return (sal)
b=selection_sort(salaries_copy)
print("sorted by selecction sort:",b)

print("top 5 higest salaries are:",a[-1:-6:-1])

